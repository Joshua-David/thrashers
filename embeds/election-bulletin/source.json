  "html": "<div class=\"election-bulletin__wrapper\"><style>.bulletin--button li,.bulletin--wrapper .story{background-size:cover;background-position:center center;list-style:none}body{padding:0;margin:0}.fc-container__header__title{padding-top:5px}section#bulletin.fc-container--thrasher{background-color:#f4f4f4;margin-bottom:0}section#bulletin.fc-container--thrasher .fc-container__inner{overflow:visible;margin:10px 0}section#bulletin.fc-container--thrasher .fc-container__inner .fc-container__body .fc-slice-wrapper .facia-snap,section#bulletin.fc-container--thrasher .fc-container__inner .fc-container__body .fc-slice-wrapper ul{overflow:visible}.bulletin--counter{position:absolute;top:10px;right:6px;color:#fff;font-family:\"Guardian Sans Web\",\"Helvetica Neue\",Helvetica,Arial,\"Lucida Grande\",sans-serif;font-size:16px;font-weight:bolder;opacity:0;z-index:4000;background-color:#4bc6df;padding:9px 6px 6px;border-radius:50%;width:20px;height:16px;text-align:center}.bulletin--counter.active{opacity:1;z-index:999999}.bulletin--counter .counter{margin-top:-4.5px;display:block}.bulletin--counter .replay{margin-top:-3px;display:none}.bulletin--counter .replay svg{width:21px;height:14px}.bulletin--counter.end .counter{display:none}.bulletin--counter.end .replay{display:block}.bulletin--wrapper{position:relative;overflow:hidden;height:100vw;background-color:#005689}.bulletin--button{width:100%;height:375px;height:100vw;position:absolute;z-index:200;-webkit-transition:all 1s ease-in-out;transition:all 1s ease-in-out}.bulletin--button.active{opacity:0}.bulletin--button li{width:100%;height:375px;height:100vw}.bulletin--button .play--button{height:30px;background-color:#E61A1A;border-radius:30px;color:#fff;font-family:\"Guardian Sans Web\",\"Helvetica Neue\",Helvetica,Arial,\"Lucida Grande\",sans-serif;margin:auto;position:absolute;top:0;bottom:0;left:0;right:0;width:90px;padding-left:12px;padding-right:12px;padding-top:5px;z-index:3000}.bulletin--button .bulletin--kicker,.bulletin--button h1{color:#fff;font-family:\"Guardian Egyptian Web\",Georgia,serif;font-weight:500;-webkit-font-smoothing:antialiased}.bulletin--button .play--button svg{width:13px;float:left;margin-right:7px;margin-left:0;margin-top:2px}.bulletin--button .headline{position:absolute;top:0;left:.58em;width:90%}.bulletin--button h1{font-size:24px;line-height:27px;display:inline;letter-spacing:-.5px;padding:2px 0 1px}.bulletin--button .bulletin--kicker{background-color:#4bc6df;box-shadow:.58em 0 0 #4bc6df,-.58em 0 0 #4bc6df;font-size:28px;line-height:29px}.bulletin--onward-feedback h1,.bulletin--wrapper .story .bulletin--kicker,.bulletin--wrapper .story h1{font-family:\"Guardian Egyptian Web\",Georgia,serif;-webkit-font-smoothing:antialiased;letter-spacing:-.5px;font-weight:500;line-height:25px}.bulletin--button .bulletin--kicker.oj{color:#005689}.bulletin--button h1{background-color:rgba(153,153,153,.7);box-shadow:.58em 0 0 rgba(153,153,153,.7),-.58em 0 0 rgba(153,153,153,.7)}.bulletin--wrapper{display:block}@media all and (min-width:61.25em){.bulletin--wrapper{display:none}}.bulletin--wrapper .story{width:100%;width:100vw;height:375px;height:100vw;padding:0;margin:0;position:absolute;opacity:0;-webkit-transition:all 1s ease-in-out;transition:all 1s ease-in-out}.bulletin--wrapper .story.active{opacity:1;z-index:2000}.bulletin--wrapper .story--copy{position:absolute;bottom:0;left:.58em;width:90%}.bulletin--wrapper .story .bulletin--kicker,.bulletin--wrapper .story h1{font-size:22px;display:inline;color:#fff;padding:2px 0 1px}.bulletin--wrapper .story .bulletin--kicker{background-color:#4bc6df;box-shadow:.58em 0 0 #4bc6df,-.58em 0 0 #4bc6df}.bulletin--wrapper .story .bulletin--kicker.oj{color:#005689}.bulletin--wrapper .story h1{background-color:rgba(153,153,153,.7);box-shadow:.58em 0 0 rgba(153,153,153,.7),-.58em 0 0 rgba(153,153,153,.7)}.bulletin--wrapper .story .clicker{position:absolute;top:0;left:0;right:0;bottom:0;z-index:3000}.bulletin--onward{margin:0 12px;padding:0;display:none;height:100vh}.bulletin--onward.active{display:block;z-index:99999999999}.bulletin--onward-feedback h1{display:inline;padding:2px 0 1px;width:85%;font-size:24px;color:#fff}.bulletin--onward-feedback p{font-family:\"Guardian Sans Web\",\"Helvetica Neue\",Helvetica,Arial,\"Lucida Grande\",sans-serif;color:#fff;display:block;font-size:16px;line-height:18px;padding-top:5px}.bulletin--onward-feedback .notification{display:none}.bulletin--onward-feedback{margin-top:110px}.bulletin--onward-feedback .reactions{display:block}.bulletin--onward-feedback .reactions svg{margin:10px 0 15px;width:40px;display:inline-block;padding-right:2px}.bulletin--onward-feedback .reactions svg:hover .thumb{fill:#fff}.bulletin--onward-feedback .reactions svg:hover .shape{fill:#4bc6df}.website--image img{width:100%}</style><!-- <div class=\"fc-container__header__title\">\n    <span>evening bulletin</span>\n</div> -->\n<div class=\"bulletin--wrapper\">\n    <div class=\"bulletin--counter\">\n        <span class=\"counter\">6</span>\n        <span class=\"replay\">\n      <svg width=\"33px\" height=\"35px\" viewBox=\"349 227 33 35\">\n          <path d=\"M364.999942,227.091015 C359.204507,227.091015 354.065888,230.027368 351.052262,234.470535 L351.245443,235.513713 L352.597712,236.325074 L353.64089,236.05462 C356.152245,232.500087 360.247685,230.143277 364.999942,230.143277 C372.61128,230.143277 378.870349,236.402346 378.870349,244.090957 C378.870349,251.74093 372.61128,257.961364 364.999942,257.961364 C360.634048,257.961364 356.886333,256.029552 354.297706,252.938654 L359.899959,252.011384 L359.899959,250.079572 L350.009084,250.079572 L349.274996,250.813661 L349.274996,261.013626 L351.168171,261.013626 L352.134077,255.179555 C355.224975,258.772725 359.784051,261.013626 364.999942,261.013626 C374.388546,261.013626 381.999884,253.440925 381.999884,244.090957 C381.999884,234.663716 374.388546,227.091015 364.999942,227.091015 L364.999942,227.091015 Z\" id=\"\" stroke=\"none\" fill=\"#FF